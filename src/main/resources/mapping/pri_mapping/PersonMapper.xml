<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mybatis.mybatis.mapper.db1.PersonMapper">

    <insert id="insertPerson" parameterType="com.mybatis.mybatis.entity.db1.Person"  useGeneratedKeys="true" keyProperty="id">

        insert  into person(id,name) values (#{id},#{name})
    </insert>

</mapper>